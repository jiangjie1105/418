<view class="page-body">
  <view class="page-section page-section-gap">
    <map id="myMap" style="width: 100%; height: 300px;" latitude="{{latitude}}" longitude="{{longitude}}" markers="{{markers}}" covers="{{covers}}" show-location></map>
  </view>
  <view class="btn-area">
    <button bindtap="getCenterLocation" class="page-body-button" type="primary">获取位置</button>
    <button bindtap="moveToLocation" class="page-body-button" type="primary">移动位置</button>
  </view>
</view>
<view class='style1'>
  <!-- <view class='horizon'></view> -->
  <view class='list1'>
    <text class='fml1'>家人一</text>
    <button class='jj' bindtap='toCall'>
      <image class='call1' src='/images/call.png'></image>
    </button>
  </view>
</view>
<!-- <view></view> -->
<view class='style2'>
  <view class='list1'>
    <text class='fml2'>家人二</text>
    <button class='jj2'>
      <image class='call2' src='/images/call.png'></image>
    </button>
  </view>
</view>